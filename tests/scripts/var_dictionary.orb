function main()

    -- simple dictionary
    var dictionary = { "key1": "value1", "key2": "value2", "key3": "value3" }

    -- assert values exist
    assert dictionary["key1"] == "value1"
    assert dictionary["key" + 2] == "value2"
    assert dictionary["key3"] == "value3"

    -- assert values can be changed
    dictionary["key1"] = "value1.1"
    assert dictionary["key1"] == "value1.1"

    -- assert values can be changed
    var dict1 = { "color": "red", "shape": "circle" }
    assert dict1["color"] == "red"

    -- assert values can be copied
    var dict2 = dict1
    assert dict2["color"] == "red"

    -- assert values can be changed
    dict2["color"] = "blue"
    assert dict2["color"] == "blue"
    assert dict1["color"] == "blue"

end