function main()

    -- set a counter
    var counter = 0

    -- set an array of numbers
    var v1 = [2, 3, 4, 5, 6, 7, 8, 9]

    -- for in loop
    for x in v1 do

        -- increment counter
        counter = counter + 1

        -- assert that x is greater than 1 and less than 10
        assertTrue(x > 1, "x should be greater than 1")

        -- assert that x is less than 10
        assertTrue(x < 10, "x should be less than 10")

    end

    -- assert that counter is 8
    assertEquals(counter, 8, "counter should be 8")

end
