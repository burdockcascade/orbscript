class vector2d
    var x = 0
    var y = 0

    function vector2d(x, y)
        self.x = x
        self.y = y
    end

end

function main()

    var v2d = new vector2d(10, 20)

    i_update_shape(v2d)

    assertEquals(v2d.x, 100, "expected x to be 100")
    assertEquals(v2d.y, 200, "expected y to be 200")

end

function i_update_shape(s)

    s.x = 100
    s.y = 200

end